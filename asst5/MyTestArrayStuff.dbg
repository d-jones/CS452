###############################################################
# readLine() - library method (class Lib)
# - reads line from standard input
# - produces String that contains the line read, except that it
#   does not include the end-of-line character-sequence. An
#   end-of-line character-sequence is one of the following:
#   - a return character followed by a newline character
#   - a newline character not preceded by a return character
#   - a return character not followed by a newline character
#   - an end-of-file character that follows at least one data
#     character
# - returns null on end-of-file
# - parameter:
#   - ($sp) - this-pointer
# - return-value:
#   - ($sp) - pointer to string containing line that was read
# - anomalies:
#   - with bare "return", looks ahead one character to check for
#     newline.  This could cause non-intuitive behavior when
#     run interactively.
###############################################################
272:readLine:
	
272#	subu $sp,$sp,8 # allocate space for data tag, saving $ra
276#	sw $ra, 4($sp) # save $ra
280#	move $t1,$sp # save "original" sp

284:  doRead:

	# read the character
284#	jal readLogicalChar

	# if we have a 'return', read another character to check for
	# newline
288#	subu $t2,$v0,13
292#	bne $t2,$zero,notReturnRL
296#	jal readLogicalChar
300#	subu $t2,$v0,10 # check for newline
304#	beq $t2,$zero,foundNewLine
308#	sw $v0,lastCharRead # push back character into queue
312#	j foundNewLine # go process the line

316:  notReturnRL:

	# at this point, $v0 has our character

316#	subu $t0,$v0,10
320#	beq $t0,$zero,foundNewLine
324#	blt $v0,$zero,foundEof
325##...slt $at,$v0,$zero
329##...bne $at,$zero,$+16

	# we have a character, so push it onto stack
332#	subu $sp,$sp,4
336#	sw $v0,($sp)

	# loop back up to get next character
340#	j doRead

344:  foundEof:
	# if we had actually read some characters before hitting
        # the eof, go return them as if a newline had been read
344#	bne $t1,$sp foundNewLine

	# otherwise, we got end of file without having read any
  # new characters, so return null
348#	sw $zero,8($sp) # return-value: null
352#	j rlReturn # return

356:  foundNewLine:

	# at this point, we have our newline (or end-of-file), and all space
        # on the stack above $t1 are characters to be put into the string.
        # That is therefore the number of data words to allocate (plus 1
        # more for the class-pointer)


	# set up GC tag and char-count on stack
356#	subu $t0,$t1,$sp # number of chars we read (times 4)
360#	srl $s6,$t0,2 # number of words on stack with chars
364#	subu $sp,$sp,4 # push char-count ...
368#	sw $s6,($sp) # ... onto stack
372#	addu $t2,$t0,5 # GC tag, incl. for count-word
376#	sw $t2,($t1) # store GC tag
	
	# allocate the appropriate Object
380#	addu $s6,$s6,7 # 3 to round up, plus 1 for v-table pointer word
384#	srl $s6,$s6,2 # data words in object
388#	move $s7,$zero # # object words in object
392#	jal newObject # allocate space
	
	# store header words
396#	la $t0,CLASS_String
400#	sw $t0,-12($s7) # store class tag (String) into object
404#	lw $t2,4($sp) # char-count
408#	sll $t1,$t2,2 # 4 times number of chars
412#	subu $t2,$zero,$t2 # negative of char-count
416#	sw $t2,-4($s7) # store negative char-count as header-word 2

	# set up pointers to various parts of stack and object
420#	lw $t0,-8($s7) # data words in object
424#	sll $t0,$t0,2 # data bytes in object
428#	subu $t0,$s7,$t0 # place to store first character (plus 8)	
432#	subu $t0,$t0,$t2 # place to store last character (plus 9)
436#	addu $sp,$sp,8 # pop redundant object-pointer and count
440#	addu $t1,$t1,$sp # first non-char spot on stack

	# at this point:
	#  $t0 points to the target-spot for the last character (plus 9)
	#  $t1 contains top spot on the stack not containing a char
	#  $sp points to the word with the last source character

	# copy the characters, popping each off the stack

444#	beq $sp,$t1,doneCharCopy
448:  charCopyLoop:
448#	lw $t2,($sp)
452#	sb $t2,-9($t0)
456#	addu $sp,$sp,4
460#	subu $t0,$t0,1
464#	bne $sp,$t1,charCopyLoop
468:  doneCharCopy:

	# put our pointer (the return value) on the eventual top stack
468#	sw $s7,8($sp)
	
472:  rlReturn:
	# restore return address and return
472#	lw $ra,4($sp) # restore return address, ...
476#	addu $sp,$sp,8 # ... by popping it (and this-pointer) off stack
480#	jr $ra
