	# - $t3 contains the value of the number we've read so far
	# - $t2 contains the value 10

764#	jal readLogicalChar # read next character
768#	subu $t1,$v0,'0' # convert digit to 0-9 value
772#	bgeu $t1,$t2,doneDigitLoop # abort if not digit
773##...sltu $at,$t1,$t2
777##...beq $at,$zero,$+20

780#	mul $t3,$t3,$t2 # multiply old value by 10
781##...mult $t3,$t2
