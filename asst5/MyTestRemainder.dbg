###############################################################
# readLine() - library method (class Lib)
# - reads line from standard input
# - produces String that contains the line read, except that it
#   does not include the end-of-line character-sequence. An
#   end-of-line character-sequence is one of the following:
#   - a return character followed by a newline character
#   - a newline character not preceded by a return character
#   - a return character not followed by a newline character
#   - an end-of-file character that follows at least one data
#     character
# - returns null on end-of-file
# - parameter:
#   - ($sp) - this-pointer
# - return-value:
#   - ($sp) - pointer to string containing line that was read
# - anomalies:
#   - with bare "return", looks ahead one character to check for
#     newline.  This could cause non-intuitive behavior when
#     run interactively.
###############################################################
448:readLine:
	
448#	subu $sp,$sp,8 # allocate space for data tag, saving $ra
452#	sw $ra, 4($sp) # save $ra
456#	move $t1,$sp # save "original" sp

460:  doRead:

	# read the character
460#	jal readLogicalChar

	# if we have a 'return', read another character to check for
	# newline
464#	subu $t2,$v0,13
468#	bne $t2,$zero,notReturnRL
472#	jal readLogicalChar
476#	subu $t2,$v0,10 # check for newline
480#	beq $t2,$zero,foundNewLine
484#	sw $v0,lastCharRead # push back character into queue
488#	j foundNewLine # go process the line

492:  notReturnRL:

	# at this point, $v0 has our character

492#	subu $t0,$v0,10
496#	beq $t0,$zero,foundNewLine
500#	blt $v0,$zero,foundEof
501##...slt $at,$v0,$zero
505##...bne $at,$zero,$+16

	# we have a character, so push it onto stack
508#	subu $sp,$sp,4
512#	sw $v0,($sp)

	# loop back up to get next character
516#	j doRead

520:  foundEof:
	# if we had actually read some characters before hitting
        # the eof, go return them as if a newline had been read
520#	bne $t1,$sp foundNewLine

	# otherwise, we got end of file without having read any
  # new characters, so return null
524#	sw $zero,8($sp) # return-value: null
528#	j rlReturn # return

532:  foundNewLine:

	# at this point, we have our newline (or end-of-file), and all space
        # on the stack above $t1 are characters to be put into the string.
        # That is therefore the number of data words to allocate (plus 1
        # more for the class-pointer)


	# set up GC tag and char-count on stack
532#	subu $t0,$t1,$sp # number of chars we read (times 4)
536#	srl $s6,$t0,2 # number of words on stack with chars
540#	subu $sp,$sp,4 # push char-count ...
544#	sw $s6,($sp) # ... onto stack
548#	addu $t2,$t0,5 # GC tag, incl. for count-word
552#	sw $t2,($t1) # store GC tag
	
	# allocate the appropriate Object
556#	addu $s6,$s6,7 # 3 to round up, plus 1 for v-table pointer word
560#	srl $s6,$s6,2 # data words in object
564#	move $s7,$zero # # object words in object
568#	jal newObject # allocate space
	
	# store header words
572#	la $t0,CLASS_String
576#	sw $t0,-12($s7) # store class tag (String) into object
580#	lw $t2,4($sp) # char-count
584#	sll $t1,$t2,2 # 4 times number of chars
588#	subu $t2,$zero,$t2 # negative of char-count
592#	sw $t2,-4($s7) # store negative char-count as header-word 2

	# set up pointers to various parts of stack and object
596#	lw $t0,-8($s7) # data words in object
600#	sll $t0,$t0,2 # data bytes in object
604#	subu $t0,$s7,$t0 # place to store first character (plus 8)	
608#	subu $t0,$t0,$t2 # place to store last character (plus 9)
612#	addu $sp,$sp,8 # pop redundant object-pointer and count
616#	addu $t1,$t1,$sp # first non-char spot on stack

	# at this point:
	#  $t0 points to the target-spot for the last character (plus 9)
	#  $t1 contains top spot on the stack not containing a char
	#  $sp points to the word with the last source character

	# copy the characters, popping each off the stack

620#	beq $sp,$t1,doneCharCopy
624:  charCopyLoop:
624#	lw $t2,($sp)
628#	sb $t2,-9($t0)
632#	addu $sp,$sp,4
636#	subu $t0,$t0,1
640#	bne $sp,$t1,charCopyLoop
644:  doneCharCopy:

	# put our pointer (the return value) on the eventual top stack
644#	sw $s7,8($sp)
	
648:  rlReturn:
	# restore return address and return
648#	lw $ra,4($sp) # restore return address, ...
652#	addu $sp,$sp,8 # ... by popping it (and this-pointer) off stack
656#	jr $ra
