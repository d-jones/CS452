	.text

###############################################################
# readLine() - library method (class Lib)
# - reads line from standard input
# - produces String that contains the line read, except that it
#   does not include the end-of-line character-sequence. An
#   end-of-line character-sequence is one of the following:
#   - a return character followed by a newline character
#   - a newline character not preceded by a return character
#   - a return character not followed by a newline character
#   - an end-of-file character that follows at least one data
#     character
# - returns null on end-of-file
# - parameter:
#   - ($sp) - this-pointer
# - return-value:
#   - ($sp) - pointer to string containing line that was read
# - anomalies:
#   - with bare "return", looks ahead one character to check for
#     newline.  This could cause non-intuitive behavior when
#     run interactively.
###############################################################
412:readLine:
	
412#	subu $sp,$sp,8 # allocate space for data tag, saving $ra
416#	sw $ra, 4($sp) # save $ra
420#	move $t1,$sp # save "original" sp

424:  doRead:

	# read the character
424#	jal readLogicalChar

	# if we have a 'return', read another character to check for
	# newline
428#	subu $t2,$v0,13
432#	bne $t2,$zero,notReturnRL
436#	jal readLogicalChar
440#	subu $t2,$v0,10 # check for newline
444#	beq $t2,$zero,foundNewLine
448#	sw $v0,lastCharRead # push back character into queue
452#	j foundNewLine # go process the line

456:  notReturnRL:

	# at this point, $v0 has our character

456#	subu $t0,$v0,10
460#	beq $t0,$zero,foundNewLine
464#	blt $v0,$zero,foundEof
465##...slt $at,$v0,$zero
469##...bne $at,$zero,$+16

	# we have a character, so push it onto stack
472#	subu $sp,$sp,4
476#	sw $v0,($sp)

	# loop back up to get next character
480#	j doRead

484:  foundEof:
	# if we had actually read some characters before hitting
        # the eof, go return them as if a newline had been read
484#	bne $t1,$sp foundNewLine

	# otherwise, we got end of file without having read any
  # new characters, so return null
488#	sw $zero,8($sp) # return-value: null
492#	j rlReturn # return

496:  foundNewLine:

	# at this point, we have our newline (or end-of-file), and all space
        # on the stack above $t1 are characters to be put into the string.
        # That is therefore the number of data words to allocate (plus 1
        # more for the class-pointer)


	# set up GC tag and char-count on stack
496#	subu $t0,$t1,$sp # number of chars we read (times 4)
500#	srl $s6,$t0,2 # number of words on stack with chars
504#	subu $sp,$sp,4 # push char-count ...
508#	sw $s6,($sp) # ... onto stack
512#	addu $t2,$t0,5 # GC tag, incl. for count-word
516#	sw $t2,($t1) # store GC tag
	
	# allocate the appropriate Object
520#	addu $s6,$s6,7 # 3 to round up, plus 1 for v-table pointer word
524#	srl $s6,$s6,2 # data words in object
528#	move $s7,$zero # # object words in object
532#	jal newObject # allocate space
	
	# store header words
536#	la $t0,CLASS_String
540#	sw $t0,-12($s7) # store class tag (String) into object
544#	lw $t2,4($sp) # char-count
548#	sll $t1,$t2,2 # 4 times number of chars
552#	subu $t2,$zero,$t2 # negative of char-count
556#	sw $t2,-4($s7) # store negative char-count as header-word 2

	# set up pointers to various parts of stack and object
560#	lw $t0,-8($s7) # data words in object
564#	sll $t0,$t0,2 # data bytes in object
568#	subu $t0,$s7,$t0 # place to store first character (plus 8)	
572#	subu $t0,$t0,$t2 # place to store last character (plus 9)
576#	addu $sp,$sp,8 # pop redundant object-pointer and count
580#	addu $t1,$t1,$sp # first non-char spot on stack

	# at this point:
	#  $t0 points to the target-spot for the last character (plus 9)
	#  $t1 contains top spot on the stack not containing a char
	#  $sp points to the word with the last source character

	# copy the characters, popping each off the stack

584#	beq $sp,$t1,doneCharCopy
588:  charCopyLoop:
588#	lw $t2,($sp)
592#	sb $t2,-9($t0)
596#	addu $sp,$sp,4
600#	subu $t0,$t0,1
604#	bne $sp,$t1,charCopyLoop
608:  doneCharCopy:

	# put our pointer (the return value) on the eventual top stack
608#	sw $s7,8($sp)
	
612:  rlReturn:
	# restore return address and return
612#	lw $ra,4($sp) # restore return address, ...
616#	addu $sp,$sp,8 # ... by popping it (and this-pointer) off stack
620#	jr $ra

###################################################################
# readInt() - library method (class Lib)
# - skips whitespace
# - then attempts to read a base-10 integer from standard input
# - aborts program if a valid integer is not found
# - returns the integer that is read
# - truncates on overflow
# - parameter:
#   - ($sp) - this-pointer
# - return-value:
#   - ($sp) - value that was read
#   - 4($sp) - (dummy) GC tag
###################################################################
624:readInt:

	# save $ra by pushing onto stack
624#	subu $sp,$sp,4
628#	sw $ra,($sp)

632:  riSkipWhiteLoop:
	# read a character
632#	jal readLogicalChar

	# if character <= 32 ascii, check for whitespace; if not
	# whitespace, abort 
636#	subu $t0,$v0,32
640#	bgt $t0,$zero,nonWhite
641##...slt $at,$zero,$t0
645##...bne $at,$zero,$+44
648#	beq $t0,$zero,riSkipWhiteLoop # start over if space
652#	subu $t0,$v0,10
656#	beq $t0,$zero,riSkipWhiteLoop # start over if newline
660#	subu $t0,$v0,9
664#	beq $t0,$zero,riSkipWhiteLoop # start over if tab
668#	subu $t0,$v0,13
672#	beq $t0,$zero,riSkipWhiteLoop # start over if carriage return
676#	subu $t0,$v0,12
680#	beq $t0,$zero,riSkipWhiteLoop # start over if form-feed
684#	j badIntegerFormat # illegal integer char: abort program

688:  nonWhite:
688#	subu $t0,$v0,'-'
692#	li $t4,1 # final multiplier
696#	bne $t0,$zero,helpReadInt # go read
700#	li $t4,-1 # -1 in final multiplier

	# read another character to make up for the '-'
704#	jal readLogicalChar

708:  helpReadInt:
708#	li $t2,10
712#	subu $t1,$v0,'0' # convert digit to 0-9 value
716#	bgeu $t1,$t2,badIntegerFormat # abort if not digit
717##...sltu $at,$t1,$t2
721##...beq $at,$zero,$+2080
724#	move $t3,$t1 #

	#### at this point, $t3 contains value of the first digit read,
	#### and $t2 contains the value 10

728:  digitLoop:
	# loop invariants:
	# - $t3 contains the value of the number we've read so far
	# - $t2 contains the value 10

728#	jal readLogicalChar # read next character
732#	subu $t1,$v0,'0' # convert digit to 0-9 value
736#	bgeu $t1,$t2,doneDigitLoop # abort if not digit
737##...sltu $at,$t1,$t2
741##...beq $at,$zero,$+20

744#	mul $t3,$t3,$t2 # multiply old value by 10
745##...mult $t3,$t2
749##...mflo $t3
752#	addu $t3,$t3,$t1 # add in value of new digit
756#	j digitLoop

760:  doneDigitLoop:

	# "push back" unused character into queue
760#	sw $v0,lastCharRead

