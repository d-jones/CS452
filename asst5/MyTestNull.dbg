
###############################################################
# readLine() - library method (class Lib)
# - reads line from standard input
# - produces String that contains the line read, except that it
#   does not include the end-of-line character-sequence. An
#   end-of-line character-sequence is one of the following:
#   - a return character followed by a newline character
#   - a newline character not preceded by a return character
#   - a return character not followed by a newline character
#   - an end-of-file character that follows at least one data
#     character
# - returns null on end-of-file
# - parameter:
#   - ($sp) - this-pointer
# - return-value:
#   - ($sp) - pointer to string containing line that was read
# - anomalies:
#   - with bare "return", looks ahead one character to check for
#     newline.  This could cause non-intuitive behavior when
#     run interactively.
###############################################################
304:readLine:
	
304#	subu $sp,$sp,8 # allocate space for data tag, saving $ra
308#	sw $ra, 4($sp) # save $ra
312#	move $t1,$sp # save "original" sp

316:  doRead:

	# read the character
316#	jal readLogicalChar

	# if we have a 'return', read another character to check for
	# newline
320#	subu $t2,$v0,13
324#	bne $t2,$zero,notReturnRL
328#	jal readLogicalChar
332#	subu $t2,$v0,10 # check for newline
336#	beq $t2,$zero,foundNewLine
340#	sw $v0,lastCharRead # push back character into queue
344#	j foundNewLine # go process the line

348:  notReturnRL:

	# at this point, $v0 has our character

348#	subu $t0,$v0,10
352#	beq $t0,$zero,foundNewLine
356#	blt $v0,$zero,foundEof
357##...slt $at,$v0,$zero
361##...bne $at,$zero,$+16

	# we have a character, so push it onto stack
364#	subu $sp,$sp,4
368#	sw $v0,($sp)

	# loop back up to get next character
372#	j doRead

376:  foundEof:
	# if we had actually read some characters before hitting
        # the eof, go return them as if a newline had been read
376#	bne $t1,$sp foundNewLine

	# otherwise, we got end of file without having read any
  # new characters, so return null
380#	sw $zero,8($sp) # return-value: null
384#	j rlReturn # return

388:  foundNewLine:

	# at this point, we have our newline (or end-of-file), and all space
        # on the stack above $t1 are characters to be put into the string.
        # That is therefore the number of data words to allocate (plus 1
        # more for the class-pointer)


	# set up GC tag and char-count on stack
388#	subu $t0,$t1,$sp # number of chars we read (times 4)
392#	srl $s6,$t0,2 # number of words on stack with chars
396#	subu $sp,$sp,4 # push char-count ...
400#	sw $s6,($sp) # ... onto stack
404#	addu $t2,$t0,5 # GC tag, incl. for count-word
408#	sw $t2,($t1) # store GC tag
	
	# allocate the appropriate Object
412#	addu $s6,$s6,7 # 3 to round up, plus 1 for v-table pointer word
416#	srl $s6,$s6,2 # data words in object
420#	move $s7,$zero # # object words in object
424#	jal newObject # allocate space
	
	# store header words
428#	la $t0,CLASS_String
432#	sw $t0,-12($s7) # store class tag (String) into object
436#	lw $t2,4($sp) # char-count
440#	sll $t1,$t2,2 # 4 times number of chars
444#	subu $t2,$zero,$t2 # negative of char-count
448#	sw $t2,-4($s7) # store negative char-count as header-word 2

	# set up pointers to various parts of stack and object
452#	lw $t0,-8($s7) # data words in object
456#	sll $t0,$t0,2 # data bytes in object
460#	subu $t0,$s7,$t0 # place to store first character (plus 8)	
464#	subu $t0,$t0,$t2 # place to store last character (plus 9)
468#	addu $sp,$sp,8 # pop redundant object-pointer and count
472#	addu $t1,$t1,$sp # first non-char spot on stack

	# at this point:
	#  $t0 points to the target-spot for the last character (plus 9)
	#  $t1 contains top spot on the stack not containing a char
	#  $sp points to the word with the last source character

	# copy the characters, popping each off the stack

476#	beq $sp,$t1,doneCharCopy
480:  charCopyLoop:
480#	lw $t2,($sp)
484#	sb $t2,-9($t0)
488#	addu $sp,$sp,4
492#	subu $t0,$t0,1
496#	bne $sp,$t1,charCopyLoop
500:  doneCharCopy:

	# put our pointer (the return value) on the eventual top stack
500#	sw $s7,8($sp)
	
504:  rlReturn:
	# restore return address and return
504#	lw $ra,4($sp) # restore return address, ...
508#	addu $sp,$sp,8 # ... by popping it (and this-pointer) off stack
512#	jr $ra

###################################################################
# readInt() - library method (class Lib)
# - skips whitespace
# - then attempts to read a base-10 integer from standard input
# - aborts program if a valid integer is not found
# - returns the integer that is read
# - truncates on overflow
# - parameter:
#   - ($sp) - this-pointer
# - return-value:
#   - ($sp) - value that was read
#   - 4($sp) - (dummy) GC tag
###################################################################
516:readInt:

	# save $ra by pushing onto stack
516#	subu $sp,$sp,4
520#	sw $ra,($sp)

524:  riSkipWhiteLoop:
	# read a character
524#	jal readLogicalChar

	# if character <= 32 ascii, check for whitespace; if not
	# whitespace, abort 
528#	subu $t0,$v0,32
532#	bgt $t0,$zero,nonWhite
533##...slt $at,$zero,$t0
537##...bne $at,$zero,$+44
540#	beq $t0,$zero,riSkipWhiteLoop # start over if space
544#	subu $t0,$v0,10
548#	beq $t0,$zero,riSkipWhiteLoop # start over if newline
552#	subu $t0,$v0,9
556#	beq $t0,$zero,riSkipWhiteLoop # start over if tab
560#	subu $t0,$v0,13
564#	beq $t0,$zero,riSkipWhiteLoop # start over if carriage return
568#	subu $t0,$v0,12
572#	beq $t0,$zero,riSkipWhiteLoop # start over if form-feed
576#	j badIntegerFormat # illegal integer char: abort program

580:  nonWhite:
580#	subu $t0,$v0,'-'
584#	li $t4,1 # final multiplier
588#	bne $t0,$zero,helpReadInt # go read
592#	li $t4,-1 # -1 in final multiplier

	# read another character to make up for the '-'
596#	jal readLogicalChar

600:  helpReadInt:
600#	li $t2,10
604#	subu $t1,$v0,'0' # convert digit to 0-9 value
608#	bgeu $t1,$t2,badIntegerFormat # abort if not digit
609##...sltu $at,$t1,$t2
613##...beq $at,$zero,$+2080
616#	move $t3,$t1 #

	#### at this point, $t3 contains value of the first digit read,
	#### and $t2 contains the value 10

620:  digitLoop:
	# loop invariants:
	# - $t3 contains the value of the number we've read so far
	# - $t2 contains the value 10

620#	jal readLogicalChar # read next character
624#	subu $t1,$v0,'0' # convert digit to 0-9 value
628#	bgeu $t1,$t2,doneDigitLoop # abort if not digit
629##...sltu $at,$t1,$t2
633##...beq $at,$zero,$+20

636#	mul $t3,$t3,$t2 # multiply old value by 10
637##...mult $t3,$t2
641##...mflo $t3
644#	addu $t3,$t3,$t1 # add in value of new digit
648#	j digitLoop

652:  doneDigitLoop:

	# "push back" unused character into queue
652#	sw $v0,lastCharRead

	# restore return address and overwrite it with return-val;
	# write dummy GC tag as second word of return-val
656#	lw $ra,($sp)
660#	mult $t3,$t4 # multiply to account for poss. minus sign
664#	mflo $t3
668#	sw $t3,($sp) # result
672#	sw $s5,4($sp) # dummy GC tag
	#lw $zero,4($sp)#**"" #--FOR MEMORY TAGGING
	
	# return
676#	jr $ra
