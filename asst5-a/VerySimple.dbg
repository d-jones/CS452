168#	subu $t0,$t1,$sp # number of chars we read (times 4)
172#	srl $s6,$t0,2 # number of words on stack with chars
176#	subu $sp,$sp,4 # push char-count ...
180#	sw $s6,($sp) # ... onto stack
184#	addu $t2,$t0,5 # GC tag, incl. for count-word
188#	sw $t2,($t1) # store GC tag
	
	# allocate the appropriate Object
192#	addu $s6,$s6,7 # 3 to round up, plus 1 for v-table pointer word
196#	srl $s6,$s6,2 # data words in object
200#	move $s7,$zero # # object words in object
204#	jal newObject # allocate space
	
	# store header words
208#	la $t0,CLASS_String
212#	sw $t0,-12($s7) # store class tag (String) into object
216#	lw $t2,4($sp) # char-count
220#	sll $t1,$t2,2 # 4 times number of chars
224#	subu $t2,$zero,$t2 # negative of char-count
228#	sw $t2,-4($s7) # store negative char-count as header-word 2
