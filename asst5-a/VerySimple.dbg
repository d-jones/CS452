2365##...bne $at,$zero,$+8
2368#	la $t2,CLASS_Object
	
	# get vtable address of object, abort if less than
	# lower limit or greater then or equal to higher
	# limit
2372:  skipArrayInstanceOf:
2372#	sge $t0,$t2,$t0
2373##...slt $t0,$t2,$t0
2377##...xori $t0,$t0,0x1
2380#	slt $t1,$t2,$t1
