#      object is a member
#  - side-effects: clobbers $t2 and $t3
########################################################
2344:instanceOf:
	# get object, go return false if null
2344#	lw $t2,($sp)
2348#	beq $t2,$zero,doneInstanceOf
	
	# get vtable address of object.  If it's an array
	# (which would be H1 <= 0), use vtable address for Object
2352#	lw $t3,-8($t2) # <= 0 if array
2356#	lw $t2,-12($t2) # vtable address (unless array)
2360#	bgt $t3,$zero,skipArrayInstanceOf
